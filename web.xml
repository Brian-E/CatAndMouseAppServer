		<servlet-class>com.catandmouseapp.GetGamesServlet</servlet-class></servlet>
	<servlet-mapping>
		<servlet-name>GetGamesServlet</servlet-name>
		<url-pattern>/getgames</url-pattern></servlet-mapping>
	<servlet>
		<servlet-name>GetNotificationsServlet</servlet-name>
		<servlet-class>com.catandmouseapp.GetNotificationsServlet</servlet-class></servlet>
	<servlet-mapping>
		<servlet-name>GetNotificationsServlet</servlet-name>
		<url-pattern>/getnotifications</url-pattern></servlet-mapping>
