		<servlet-class>com.catandmouseapp.UpdateLocationServlet</servlet-class></servlet>
	<servlet-mapping>
		<servlet-name>UpdateLocationServlet</servlet-name>
		<url-pattern>/updatelocation</url-pattern></servlet-mapping>
	<servlet>
		<servlet-name>CreateGameServlet</servlet-name>
		<servlet-class>com.catandmouseapp.CreateGameServlet</servlet-class></servlet>
	<servlet-mapping>
		<servlet-name>CreateGameServlet</servlet-name>
		<url-pattern>/creategame</url-pattern></servlet-mapping>
	<servlet>
		<servlet-name>PlayerLocationsServlet</servlet-name>
		<servlet-class>com.catandmouseapp.PlayerLocationsServlet</servlet-class></servlet>
	<servlet-mapping>
		<servlet-name>PlayerLocationsServlet</servlet-name>
		<url-pattern>/playerlocations</url-pattern></servlet-mapping>
