		<servlet-class>com.catandmouseapp.JoinGameServlet</servlet-class></servlet>
	<servlet-mapping>
		<servlet-name>JoinGameServlet</servlet-name>
		<url-pattern>/joingame</url-pattern></servlet-mapping>
	<servlet>
		<servlet-name>QuitGameServlet</servlet-name>
		<servlet-class>com.catandmouseapp.QuitGameServlet</servlet-class></servlet>
	<servlet-mapping>
		<servlet-name>QuitGameServlet</servlet-name>
		<url-pattern>/quitgame</url-pattern></servlet-mapping>
	<servlet><servlet-name>PlayerCleanupTaskServlet</servlet-name>
		<servlet-class>com.catandmouseapp.PlayerCleanupTaskServlet</servlet-class></servlet>
	<servlet-mapping>
		<servlet-name>PlayerCleanupTaskServlet</servlet-name>
		<url-pattern>/cron/playercleanup</url-pattern></servlet-mapping>
